{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .ts",
    "build": "tsc",
    "serve": "npm run kill && npm run build && npm run build -- --watch | firebase emulators:start --import ./localdata --export-on-exit ./localdata",
    "kill": "lsof -t -i :9099 -i:5001 -i:8080 -i:9199 -i:4000 -i:4400 | xargs kill -9",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "14"
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "^9.11.0",
    "firebase-functions": "^3.15.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.24.0",
    "firebase-functions-test": "^0.3.2",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "private": true
}
